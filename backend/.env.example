# FlowMind Analytics - Environment Variables Template
# Copy to .env and set actual values

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173

# TradeStation API Configuration
TS_BASE_URL=https://api.tradestation.com
TS_AUTH_URL=https://api.tradestation.com/authorize
TS_TOKEN_URL=https://api.tradestation.com/oauth/token
TS_SCOPE=openid offline_access
TS_HTTP_TIMEOUT=15
TS_REFRESH_SKEW=60
TS_REDIRECT_URI=http://localhost:8000/api/ts/callback

# TradeStation OAuth Credentials (set in secure vault)
TRADESTATION_API_KEY="<set in vault>"
TRADESTATION_API_SECRET="<set in vault>"
TS_CLIENT_SECRET="<set in vault>"

# Unusual Whales API
UW_API_TOKEN="<set in vault>"

# Database
MONGO_URL="mongodb://localhost:27017/flowmind"

# Application
ENV="development"
DEBUG="true"