{
  "long_call": {
    "id": "long_call",
    "name": "Long Call",
    "category": "bullish",
    "complexity": "beginner",
    "description": "Buy a call option",
    "legs": [
      {
        "type": "call",
        "action": "buy",
        "quantity": 1,
        "strike_offset": 0
      }
    ],
    "pnl_formula": {
      "type": "simple",
      "below_strike": "-premium",
      "above_strike": "(price - strike) * 100 - premium"
    },
    "breakeven": "strike + (premium / 100)",
    "max_profit": "unlimited",
    "max_loss": "premium",
    "ideal_conditions": {
      "market_view": "Strongly bullish",
      "volatility": "Low to moderate",
      "time_horizon": "Medium to long term"
    }
  },
  "short_call": {
