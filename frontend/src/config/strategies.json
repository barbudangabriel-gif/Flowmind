  "bull_call_spread": {
    "id": "bull_call_spread",
    "name": "Bull Call Spread",
    "category": "bull",
    "complexity": "intermediate",
    "description": "Limited risk bull spread with defined profit potential",
    "legs": [
      {
        "type": "call",
        "action": "buy",
        "quantity": 1,
        "strike_label": "lower"
      },
      {
        "type": "call",
        "action": "sell",
        "quantity": 1,
        "strike_label": "higher"
      }
    ],
    "pnl_formula": {
      "type": "spread",
      "description": "Net debit spread"
    },
    "breakeven": "lower_strike + net_debit / 100",
    "max_profit": "(higher_strike - lower_strike) * 100 - net_debit",
    "max_loss": "net_debit",
    "ideal_conditions": {
      "market_view": "Moderately bull",
      "volatility": "Low to moderate",
      "time_horizon": "Short to medium term"
    }
  },
