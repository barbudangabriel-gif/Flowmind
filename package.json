{
  "name": "flowmind-fis-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite --host 0.0.0.0 --port 5173 --strictPort --open=false --clearScreen=false",
    "start:staging": "vite --host 0.0.0.0 --port 5173 --strictPort",
    "mock:api": "nodemon --signal SIGTERM scripts/mock-api.js",
    "dev:all": "concurrently -k -n API,UI \"yarn mock:api\" \"yarn start\"",
    "smoke:fis": "concurrently -k -n API,UI \"yarn mock:api\" \"yarn ui\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.0.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
